{
    "version": 2,
    "builds": [
      {
        "src": "src/index.js",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "build"
        }
      }
    ],
    "routes": [
      {
        "src": "/",
        "dest": "/index.html"
      },
      {
        "src": "/signup",
        "dest": "/index.html"
      },
      {
        "src": "/chat",
        "dest": "/index.html"
      },
      {
        "src": "/group-chat",
        "dest": "/index.html"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }
  